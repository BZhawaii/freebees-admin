

  <ul class="list-group">
    {{#each establishmentsGot}}
      <a href="/establishments/{{id}}" class="list-group-item">

        <h1>Name: {{name}}</h1>
        <h4>Address: {{address}}</h4>
        <h4>Website:  {{www}}</h4>
        <h4>About: {{about}}</h4>
        <img style="height:300px;width:300px"src="{{imageURL}}" alt="this is an image of {{imageURL}}">
      </a>
    {{/each}}
  </ul>


  <a type="button" class="btn btn-default btn-primary" href="/establishments/new">New establishment</a>
